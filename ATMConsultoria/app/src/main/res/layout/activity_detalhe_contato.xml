<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DetalheContatoActivity" tools:layout_editor_absoluteY="25dp">

    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/detalhe_contato"
            android:id="@+id/imageView5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Contatos"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:id="@+id/textView13"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBottom_toBottomOf="@+id/imageView5"
            app:layout_constraintTop_toTopOf="@+id/imageView5" app:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Contato 01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView14"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" app:layout_constraintStart_toStartOf="@+id/textView13"
    />
    <TextView
            android:text="Contato 02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            app:layout_constraintStart_toStartOf="@+id/textView14"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14"/>
    <TextView
            android:text="Contato 03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            app:layout_constraintStart_toStartOf="@+id/textView12"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12"/>
</android.support.constraint.ConstraintLayout>